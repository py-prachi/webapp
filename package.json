{
  "name": "webapp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --forceExit",
    "inttest": "npm run migration:run && jest --config=jest.inttest.config.js",
    "inttest:ci": "npm run docker-compose-run npm run inttest && npm run docker-compose-down",
    "e2etest": "npm run migration:run && jest --config=jest.e2etest.config.js --forceExit",
    "e2etest:ci": "npm run docker-compose-run npm run e2etest && npm run docker-compose-down",
    "docker-compose-run": "docker-compose -p webapp run test",
    "docker-compose-down": "docker-compose -p webapp down --remove-orphans",
    "test:watch": "jest --watch",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "start": "node -r dotenv/config --enable-source-maps dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs -d database/ormconfig.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:show": "npm run typeorm migration:show"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  }
}
